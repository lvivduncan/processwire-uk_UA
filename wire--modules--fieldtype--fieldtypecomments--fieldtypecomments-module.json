        "924daa0b046449bd4fb99af3c97b5f2b": {
            "text": "\u0429\u043e\u0431 \u0437\u043c\u0456\u043d\u0438\u0442\u0438 \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u0443 \u0437\u0456\u0440\u043e\u0447\u043a\u0443, \u0449\u043e \u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u043e\u0432\u0443\u0454\u0442\u044c\u0441\u044f \u0434\u043b\u044f \u0432\u0438\u0432\u043e\u0434\u0443 (HTML \u0442\u0435\u0436 \u043c\u043e\u0436\u043d\u0430):"
        },
        "359310b90581d6b9baa3cdccc10607b2": {
            "text": "\u0422\u0430\u043a (\u0437\u0456\u0440\u043a\u043e\u0432\u0438\u0439 \u0440\u0435\u0439\u0442\u0438\u043d\u0433 \u043d\u0435\u043e\u0431\u043e\u0432'\u044f\u0437\u043a\u043e\u0432\u0438\u0439)"
        },
        "806e88271605e15a20e9d6d47673f42e": {
            "text": "\u0422\u0430\u043a (\u0437\u0456\u0440\u043a\u043e\u0432\u0438\u0439 \u0440\u0435\u0439\u0442\u0438\u043d\u0433 \u043e\u0431\u043e\u0432\u2019\u044f\u0437\u043a\u043e\u0432\u0438\u0439)"
        },
        "b70aad8c6cb77fde247be00630d7f5ef": {
            "text": "G: \u041f\u0456\u0434\u0445\u043e\u0434\u0435 \u0434\u043b\u044f \u0432\u0456\u0434\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u043d\u044f \u043d\u0430 \u0432\u0441\u0456\u0445 \u0432\u0435\u0431-\u0441\u0430\u0439\u0442\u0430\u0445 \u0437 \u0431\u0443\u0434\u044c-\u044f\u043a\u0438\u043c \u0442\u0438\u043f\u043e\u043c \u0430\u0443\u0434\u0438\u0442\u043e\u0440\u0456\u0457."
        },
        "8e25a98f71c3244890fa8ecf2230fe18": {
            "text": "PG: \u041c\u043e\u0436\u0435 \u043c\u0456\u0441\u0442\u0438\u0442\u0438 \u0433\u0440\u0443\u0431\u0456 \u0436\u0435\u0441\u0442\u0438, \u043f\u0440\u043e\u0432\u043e\u043a\u0430\u0446\u0456\u0439\u043d\u043e \u043e\u0434\u044f\u0433\u043d\u0435\u043d\u0438\u0445 \u043e\u0441\u0456\u0431, \u043c\u0435\u043d\u0448 \u043b\u0430\u0439\u043b\u0438\u0432\u0456 \u0441\u043b\u043e\u0432\u0430 \u0430\u0431\u043e \u043b\u0435\u0433\u043a\u0435 \u043d\u0430\u0441\u0438\u043b\u044c\u0441\u0442\u0432\u043e."
        },
        "2900074213d129e33d5d4b6e74076f29": {
            "text": "R: \u041c\u043e\u0436\u0435 \u043c\u0456\u0441\u0442\u0438\u0442\u0438 \u0442\u0430\u043a\u0456 \u0440\u0435\u0447\u0456, \u044f\u043a \u0431\u0440\u0443\u0434\u043d\u0430 \u043d\u0435\u043d\u043e\u0440\u043c\u0430\u0442\u0438\u0432\u043d\u0430 \u043b\u0435\u043a\u0441\u0438\u043a\u0430, \u0456\u043d\u0442\u0435\u043d\u0441\u0438\u0432\u043d\u0435 \u043d\u0430\u0441\u0438\u043b\u044c\u0441\u0442\u0432\u043e, \u043e\u0433\u043e\u043b\u0435\u043d\u0435 \u0442\u0456\u043b\u043e, \u0430\u0431\u043e \u0432\u0436\u0438\u0432\u0430\u043d\u043d\u044f \u0442\u044f\u0436\u043a\u0438\u0445 \u043d\u0430\u0440\u043a\u043e\u0442\u0438\u043a\u0456\u0432."
        },
        "043cab853d189bb505d101c3c7ab3176": {
            "text": "X: \u041c\u043e\u0436\u0435 \u043c\u0456\u0441\u0442\u0438\u0442\u0438 \u0432\u0430\u0436\u043a\u0456 \u0441\u0435\u043a\u0441\u0443\u0430\u043b\u044c\u043d\u0456 \u043e\u0431\u0440\u0430\u0437\u0438 \u0447\u0438 \u043d\u0430\u0434\u0437\u0432\u0438\u0447\u0430\u0439\u043d\u043e \u0442\u0440\u0438\u0432\u043e\u0436\u043d\u0435 \u043d\u0430\u0441\u0438\u043b\u044c\u0441\u0442\u0432\u043e."
        },
        "a4949d12f429ca4af2feb4e30c1e84e4": {
            "text": "\u0412\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u043e\u0432\u0443\u0432\u0430\u0442\u0438 Gravatar?"
        },
        "c5fce56aee82d2bc007cbf213998945b": {
            "text": "\u0426\u044f \u0441\u043b\u0443\u0436\u0431\u0430 \u043d\u0430\u0434\u0430\u0454 \u0430\u0432\u0430\u0442\u0430\u0440\u043d\u0435 \u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u043d\u044f \u0437 \u043a\u043e\u0436\u043d\u0438\u043c \u043a\u043e\u043c\u0435\u043d\u0442\u0430\u0440\u0435\u043c (\u0443\u043d\u0456\u043a\u0430\u043b\u044c\u043d\u0435 \u0434\u043b\u044f \u0430\u0434\u0440\u0435\u0441\u0438 \u0435\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0457 \u043f\u043e\u0448\u0442\u0438). \u0429\u043e\u0431 \u0443\u0432\u0456\u043c\u043a\u043d\u0443\u0442\u0438, \u0432\u0438\u0431\u0435\u0440\u0456\u0442\u044c \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u0443 \u043e\u0446\u0456\u043d\u043a\u0443 gravatar. \u0426\u0435 \u0442\u0435 \u0436 \u0441\u0430\u043c\u0435, \u0449\u043e \u0456 \u0440\u0435\u0439\u0442\u0438\u043d\u0433\u0438 \u0444\u0456\u043b\u044c\u043c\u0456\u0432, \u0434\u0435 G \u0454 \u043d\u0430\u0439\u0431\u0456\u043b\u044c\u0448 \u043f\u0440\u0438\u044f\u0437\u043d\u0438\u0439 \u0434\u043e \u0441\u0456\u043c'\u0457, \u0430 X - \u043d\u0456."
        },
        "e9f6051c655ca4a6773af3f2d8458815": {
            "text": "\u0420\u0435\u0439\u0442\u0438\u043d\u0433\u043e\u0432\u0456 \u043e\u043f\u0438\u0441\u0438 \u043d\u0430\u0434\u0430\u043d\u0456 [Gravatar](https:\/\/en.gravatar.com\/site\/implement\/images\/)."
        },
        "82829c5912f395b029a87c565359835c": {
            "text": "\u0422\u0435\u043a\u0441\u0442\u043e\u0432\u0456 \u0444\u043e\u0440\u043c\u0430\u0442\u0438"
        },
        "53a682f62e65f496a659d3a12c754f75": {
            "text": "\u041d\u0435\u043e\u0431\u043e\u0432\u2019\u044f\u0437\u043a\u043e\u0432\u0430 \u043e\u043f\u0446\u0456\u044f, \u043e\u0431\u0435\u0440\u0456\u0442\u044c \u043e\u0434\u0438\u043d \u0430\u0431\u043e \u0434\u0435\u043a\u0456\u043b\u044c\u043a\u0430 \u0442\u0435\u043a\u0441\u0442\u043e\u0432\u0438\u0445 \u0444\u043e\u0440\u043c\u0430\u0442\u0456\u0432, \u044f\u043a\u0456 \u0431\u0443\u0434\u0443\u0442\u044c \u0437\u0430\u0441\u0442\u043e\u0441\u043e\u0432\u0443\u0432\u0430\u0442\u0438\u0441\u044f \u0434\u043e \u0442\u0435\u043a\u0441\u0442\u0443 \u043a\u043e\u043c\u0435\u043d\u0442\u0430\u0440\u044f \u0432 \u043e\u0431\u0440\u0430\u043d\u043e\u043c\u0443 \u043f\u043e\u0440\u044f\u0434\u043a\u0443. \u042f\u043a\u0449\u043e \u0412\u0438 \u043d\u0435 \u043e\u0431\u0435\u0440\u0435\u0442\u0435 \u0445\u043e\u0447 \u044f\u043a\u0438\u0439\u0441\u044c, \u0442\u043e \u0442\u0435\u043a\u0441\u0442 \u043a\u043e\u0434\u0443\u0432\u0430\u0442\u0438\u043c\u0435\u0442\u044c\u0441\u044f \u0441\u0443\u0442\u043d\u0456\u0441\u0442\u044e, \u043d\u043e\u0432\u0456 \u0440\u044f\u0434\u043a\u0438 \u043a\u043e\u0434\u0443\u0432\u0430\u0442\u0438\u043c\u0435 \u0443 <br> \u0442\u0435\u0433 \u0456 \u0431\u0443\u0434\u0435 \u0432\u0438\u0432\u043e\u0434\u0438\u0442\u0438\u0441\u044f \u0443 <p> \u0442\u0435\u0433\u043e\u0432\u0456."
        },
        "81958f1863ae9ebb74b7a23cbf0dbd78": {
            "text": "\u0423\u0432\u0430\u0433\u0430: \u043e\u0431\u0438\u0440\u0430\u0439\u0442\u0435 \u0442\u0456\u043b\u044c\u043a\u0438 \u0442\u0430\u043a\u0438\u0439 \u0444\u043e\u0440\u043c\u0430\u0442 \u0442\u0435\u043a\u0441\u0442\u0443, \u044f\u043a\u0438\u0439 \u0454 \u0431\u0435\u0437\u043f\u0435\u0447\u043d\u0438\u0439 \u0434\u043b\u044f \u0430\u043d\u043e\u043d\u0456\u043c\u043d\u043e\u0433\u043e \u0432\u0432\u043e\u0434\u0443, \u043d\u0430\u043f\u0440\u0438\u043a\u043b\u0430\u0434, Entity Encoder (\u044f\u0434\u0440\u043e) \u0430\u0431\u043e Textile Restricted (\u0442\u0440\u0435\u0442\u044f \u0441\u0442\u043e\u0440\u043e\u043d\u0430). \u042f\u043a\u0449\u043e \u0432\u0438 \u043d\u0435 \u0432\u043f\u0435\u0432\u043d\u0435\u043d\u0456, \u0442\u043e\u0434\u0456 \u043d\u0456\u0447\u043e\u0433\u043e \u0442\u0443\u0442 \u043d\u0435 \u0432\u0438\u0431\u0438\u0440\u0430\u0439\u0442\u0435, \u043e\u0441\u043a\u0456\u043b\u044c\u043a\u0438 \u043d\u0435\u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u0438\u0439 \u0432\u0438\u0431\u0456\u0440 \u043c\u043e\u0436\u0435 \u0441\u0442\u0430\u0442\u0438 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u043e\u044e \u0431\u0435\u0437\u043f\u0435\u043a\u0438."
        },
        "b672f52ade975e864ae6b58722c03689": {
            "text": "\u0420\u0435\u0430\u043b\u0456\u0437\u0430\u0446\u0456\u044f"
